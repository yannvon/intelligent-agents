<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### DELIBERATIVE AGENTS ### -->
	<agent name="deliberative-random">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeTemplate"/>
		<set algorithm="BFS"/>
	</agent>

	<agent name="deliberative-agent">
		<set class-path="bin/"/>
		<set class-name="deliberative.DeliberativeAgent"/>
		<set algorithm="BFS"/>
	</agent>
	<agent name="deliberative-agent-main">
		<set class-path="bin/"/>
		<set class-name="deliberative.DeliberativeAgent"/>
		<set algorithm="ASTAR"/>
	</agent>
	<agent name="deliberative-agent-heur0">
		<set class-path="bin/"/>
		<set class-name="deliberative.DeliberativeAgentHeur0"/>
		<set algorithm="ASTAR"/>
	</agent>
	
	<agent name="deliberative-agent-better">
		<set class-path="bin/"/>
		<set class-name="deliberative.DeliberativeAgentTestCostOpt"/>
		<set algorithm="BFS"/>
	</agent>
	
</agents>
