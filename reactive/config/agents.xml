<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>
	<agent name="reactive-loic">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveLoic"/>
		<set discount-factor=".50"/>
		<set it="1000.0"/>
	</agent>
	<agent name="reactive-loic-exp">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveLoic"/>
		<set discount-factor=".90"/>
		<set it="1000.0"/>
	</agent>

</agents>
