<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>
	<agent name="reactive-loic">
		<set class-path="bin/"/>
		<set class-name="qlearning.ReactiveLoic"/>
		<set discount-factor=".90"/>
	</agent>
		<agent name="reactive-loic2">
		<set class-path="bin/"/>
		<set class-name="qlearning.ReactiveLoic2"/>
		<set discount-factor=".90"/>
	</agent>
	<agent name="reactive-test">
		<set class-path="bin/"/>
		<set class-name="qlearning.ReactiveTest"/>
		<set discount-factor=".90"/>
	</agent>
	<agent name="reactive-yann">
		<set class-path="bin/"/>
		<set class-name="qlearning.ReactiveTemplateYann"/>
		<set discount-factor=".85"/>
	</agent><agent name="reactive-old">
		<set class-path="bin/"/>
		<set class-name="qlearning.ReactiveOld"/>
		<set discount-factor=".50"/>
		<set it="1000.0"/>
	</agent>

</agents>
