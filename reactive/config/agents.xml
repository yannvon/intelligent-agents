<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>
	<agent name="reactive-test">
		<set class-path="bin/"/>
		<set class-name="reactive.ReactiveTest"/>
		<set discount-factor=".99"/>
	</agent>
	<agent name="reactive-normal">
		<set class-path="bin/"/>
		<set class-name="reactive.ReactiveAgent"/>
		<set discount-factor=".99"/>
	</agent>

</agents>
