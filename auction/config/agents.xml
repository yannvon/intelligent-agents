<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### AUCTION AGENTS ### -->
	<agent name="auction-random">
		<set class-path="bin/"/>
		<set class-name="template.AuctionTemplate"/>
	</agent>
	
	<agent name="auction-destroyer">
		<set class-path="bin/"/>
		<set class-name="template.TemplateDestroyer"/>
	</agent>

	<agent name="auction-noreorder">
		<set class-path="bin/"/>
		<set class-name="auction.AuctionNoReorder"/>
	</agent>

	<agent name="auction-cplanning">
		<set class-path="bin/"/>
		<set class-name="auction.AuctionCentralizedPlanning"/>
	</agent>
	
	<agent name="auction-cplanning2">
		<set class-path="bin/"/>
		<set class-name="auction.AuctionReorderCentralized"/>
	</agent>

	<agent name="auction-decentralized">
		<set class-path="bin/"/>
		<set class-name="auction.FirstStrategy"/>
	</agent>
	
	<agent name="auction-counter">
		<set class-path="bin/"/>
		<set class-name="auction.CounterStrat"/>
	</agent>
	
	<agent name="auction-counter2">
		<set class-path="bin/"/>
		<set class-name="auction.CounterStrat2"/>
	</agent>
	
	<agent name="auction-multiAgent">
		<set class-path="bin/"/>
		<set class-name="auction.AuctionMultiAgents"/>
	</agent>

	<agent name="auction-multiweight">
		<set class-path="bin/"/>
		<set class-name="auction.AuctionMultiplicativeWeightUpdate"/>
	</agent>




</agents>
